      <key>/schemas/apps/gnome-search-tool/contains_the_text</key>
      <owner>gnome-search-tool</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
        <short>Select Contains the Text</short>
        <long>
          This key determines if the Contains the Text option is
	  selected when the search tool is started.
        </long>
      </locale>
    </schema>
    <schema>
