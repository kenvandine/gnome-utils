<?xml version="1.0"?>
<GTK-Interface>

<project>
  <name>GTTglade</name>
  <program_name>gttglade</program_name>
  <directory></directory>
  <source_directory></source_directory>
  <pixmaps_directory></pixmaps_directory>
  <language>C</language>
  <gnome_support>True</gnome_support>
  <gettext_support>True</gettext_support>
  <output_main_file>False</output_main_file>
  <output_support_files>False</output_support_files>
  <output_build_files>False</output_build_files>
  <backup_source_files>False</backup_source_files>
  <output_translatable_strings>True</output_translatable_strings>
  <translatable_strings_file>interval_popup_strings.c</translatable_strings_file>
</project>

<widget>
  <class>GtkMenu</class>
  <name>Interval Popup</name>

  <widget>
    <class>GtkMenuItem</class>
    <name>edit</name>
    <tooltip>Edit the Start &amp; Stop Times</tooltip>
    <signal>
      <name>activate</name>
      <handler>on_edit_activate</handler>
      <last_modification_time>Sat, 01 Sep 2001 04:19:16 GMT</last_modification_time>
    </signal>
    <label>Edit</label>
    <right_justify>False</right_justify>
  </widget>

  <widget>
    <class>GtkPixmapMenuItem</class>
    <name>delete</name>
    <tooltip>Delete This Interval</tooltip>
    <signal>
      <name>activate</name>
      <handler>on_delete_activate</handler>
      <last_modification_time>Sat, 01 Sep 2001 04:16:00 GMT</last_modification_time>
    </signal>
    <label>Delete</label>
    <right_justify>False</right_justify>
    <stock_icon>GNOME_STOCK_MENU_CUT</stock_icon>
  </widget>

  <widget>
    <class>GtkMenuItem</class>
    <name>separator1</name>
    <right_justify>False</right_justify>
  </widget>

  <widget>
    <class>GtkMenuItem</class>
    <name>merge_up</name>
    <tooltip>Merge Into Interval Above</tooltip>
    <signal>
      <name>activate</name>
      <handler>on_merge_up_activate</handler>
      <last_modification_time>Sat, 01 Sep 2001 04:16:00 GMT</last_modification_time>
    </signal>
    <label>Merge Up</label>
    <right_justify>False</right_justify>
  </widget>

  <widget>
    <class>GtkMenuItem</class>
    <name>merge_down</name>
    <tooltip>Merege Into Interval Below</tooltip>
    <signal>
      <name>activate</name>
      <handler>on_merge_down_activate</handler>
      <last_modification_time>Sat, 01 Sep 2001 04:18:36 GMT</last_modification_time>
    </signal>
    <label>Merge Down</label>
    <right_justify>False</right_justify>
  </widget>
</widget>

</GTK-Interface>
